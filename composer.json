{
    "name": "spywer/yii2-tarantool",
    "description": "Tarantool connector for yii2 framework. Allows to perform SQL and NoSQL queries, ActiveRecord, Schemas and more.",
    "type": "yii2-extension",
    "keywords": ["yii2"," tarantool", "db", "nosql"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "mhthnz",
            "email": "mhthnz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8",
        "yiisoft/yii2": "~2.0.35",
        "tarantool/client": "^0.10.0",
        "rybakit/msgpack": "^0.9.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7",
        "yiisoft/yii2-debug": ">=2.0.14"
    },
    "autoload": {
        "psr-4": {
            "mhthnz\\tarantool\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "mhthnz\\tarantool\\tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
